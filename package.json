{
  "name": "tgsnake",
  "version": "2.0.0-beta.0",
  "description": "Telegram MTProto framework base gram.js",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "ts-node ./tes/index.ts",
    "prepare": "husky install && echo \"ðŸ Building snake package.\" && tsc",
    "build": "tsc",
    "pretty-quick": "pretty-quick --staged --pattern \"src/**/*.*(js|jsx|ts|tsx)\"",
