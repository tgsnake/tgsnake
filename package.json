{
  "name": "tgsnake",
  "version": "2.0.0-beta.17",
  "description": "Telegram MTProto framework base gram.js",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "ts-node ./tes/index.ts --project ./tsconfig.json",
    "build": "rm -rf ./lib && tsc",
    "pretty-quick": "pretty-quick --staged --pattern \"src/**/*.*(js|jsx|ts|tsx)\"",
    "prettier": "prettier -w ./src",
    "update": "yarn up \"**\" --interactive",
    "prepare": "husky install && echo \"ðŸ Building snake package.\" && rm -rf ./lib && tsc",
    "prepublish": "yarn prepare",
    "postinstall": "yarn prepare",
    "prepack" : "yarn prepare"
  },
  "homepage": "https://tgsnake.js.org",
  "keywords": [
    "tgsnake",
    "telegram",
    "mtproto",
    "gramjs",
    "userbot"
  ],
  "bugs": "https://github.com/tgsnake/tgsnake/issues",
  "repository": "https://github.com/tgsnake/tgsnake",
  "author": "butthx",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@tgsnake/parser": "1.1.1",
    "axios": "0.26.0",
    "big-integer": "1.6.51",
    "chalk": "4.1.2",
    "file-type": "16.5.3",
    "husky": "7.0.4",
    "prompts": "2.4.2",
    "telegram": "2.5.3",
    "tg-file-id": "1.1.2",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@types/node": "17.0.13",
    "dotenv": "^16.0.0",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "ts-node": "10.4.0",
    "typedoc": "^0.22.11",
    "typescript": "4.5.5"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0"
  }
}
