    "husky": "7.0.4",
    "prompts": "2.4.2",
    "telegram": "2.0.8",
    "tg-file-id": "1.1.2"
  },
  "devDependencies": {
    "@types/node": "16.11.11",
    "dotenv": "10.0.0",
